{
  "name": "backend-e2e",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "implicitDependencies": ["backend"],
  "projectType": "application",
  "targets": {
    "start": {
      "executor": "nx:run-commands",
      "options": {
        "parallel": false,
        "command": "jest --passWithNoTests --config apps/backend-e2e/jest.config.ts"
      }
    },
    "start:docker": {
      "executor": "nx:run-commands",
      "outputs": ["{workspaceRoot}/coverage/{e2eProjectRoot}"],
      "options": {
        "parallel": false,
        "commands": [
          {
            "command": "nx start:compose backend"
          },
          {
            "command": "nx openapi:generate backend"
          },
          {
            "command": "nx start backend-e2e"
          }
        ]
      }
    }
  }
}
